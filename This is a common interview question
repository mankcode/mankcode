sentence = 'This is a common interview question'
sentece_list = []
alphabeth ='abcdefghijklmnoprstuxyz'
alphabeth_list = []
dict={}
for x in sentence:
    sentece_list.append(x)
    
for x in alphabeth:
    alphabeth_list.append(x)
    
for letter in alphabeth_list:
    dict[letter] = sentece_list.count(letter)

max=0
lista_wynikow = []
for element in dict:
    lista_wynikow.append(dict[element])
    if dict[element] > max:
        max=dict[element]

print(max)    
print(lista_wynikow)
print(lista_wynikow.index(max))
print(f'Oto litera z najwięszą częstotliwością występowania "{sentece_list[max]}". Wsytąpiła: {max} razy.')

